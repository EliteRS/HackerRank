static void staircase(int n) {
        int i,j;
        for(i=0;i<n;i++){
            for(j=0;j<n;j++){
                if(j<n-1-i){
                    System.out.print(" ");
                }else{
                    System.out.print("#");
                }
            }
            System.out.println("");
        }

    }
