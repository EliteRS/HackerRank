#include <stdio.h>
int main (){
	
	
	int a[3]={17,28,30};
	int b[3]={99,16,8};
	
	int bc=0;
	int ac=0;
	int i;
	int resultA;
	int resultB=0;
	
	for (i=0;i<3;i++){
		
		if(a[i]<b[i]){
			bc++;
			
		}else if(a[i]>b[i]){
			ac++;
			
		}else if(a[i]==b[i]){
			continue;
		}
	}
	printf ("%d %d",ac,bc);
	
	
	
}
